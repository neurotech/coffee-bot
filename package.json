{
  "name": "coffee-bot",
  "version": "1.0.0",
  "description": "Coffee anyone?",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "buildsrc": "parcel build src/index.html --out-dir build --public-url ./",
    "predeploy": "rm -rf build/ && set NODE_ENV=production&&npm run buildsrc",
    "deploy": "now --public --local-config now.json && now alias --local-config now.json",
    "clean": "now --local-config now.json rm coffee-bot --safe --yes",
    "test": "node test",
    "watch": "parcel watch src/index.html --out-dir build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neurotech/coffee-bot.git"
  },
  "author": "Tim Douglas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/neurotech/coffee-bot/issues"
  },
  "homepage": "https://github.com/neurotech/coffee-bot#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cryptr": "^4.0.2",
    "dion": "^2.0.0",
    "dotenv": "^6.2.0",
    "log-symbols": "^2.2.0",
    "request-data": "^1.2.0",
    "righto": "^5.1.0",
    "sea-lion": "^2.0.2",
    "tiny-json-http": "^7.0.2"
  },
  "devDependencies": {
    "cpjax": "^1.1.3",
    "fastn": "^2.12.0",
    "normalize.css": "^8.0.1",
    "parcel": "^1.11.0",
    "tape": "^4.9.1"
  }
}
